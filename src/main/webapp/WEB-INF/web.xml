<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>fileServlet</servlet-name>
    <servlet-class>com.main.servlet.MainServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>logOutServlet</servlet-name>
    <servlet-class>com.main.servlet.LogOutServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RegServlet</servlet-name>
    <servlet-class>com.main.servlet.RegistrationServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LogServlet</servlet-name>
    <servlet-class>com.main.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>registration</servlet-name>
    <jsp-file>/pages/registration.html</jsp-file>
  </servlet>

  <servlet-mapping>
    <servlet-name>fileServlet</servlet-name>
    <url-pattern>/files/</url-pattern>
  </servlet-mapping>logOutServlet

  <servlet-mapping>
    <servlet-name>logOutServlet</servlet-name>
    <url-pattern>/back/account/out/</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>RegServlet</servlet-name>
    <url-pattern>/back/account/reg/</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>LogServlet</servlet-name>
    <url-pattern>/back/account/log/</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>registration</servlet-name>
    <url-pattern>/registration/</url-pattern>
  </servlet-mapping>
</web-app>
